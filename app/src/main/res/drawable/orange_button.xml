<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape >
            <gradient
                android:startColor="#F57C00"
                android:endColor="#F57C00"
                android:angle="270" />

        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="@color/orangeButton"
                android:endColor="@color/orangeButton"
                android:angle="270" />
            <stroke
                android:color="@color/strokeOrangeButton"
                android:width="5dp"/>
        </shape>
    </item>
</selector>
